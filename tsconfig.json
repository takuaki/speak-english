{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*.ts", "src/**/*.vue", "types/*.ts"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "target": "esnext",
    "module": "esnext",
    "isolatedModules": true,
    "allowJs": false,
    "typeRoots": ["./note_modules/@types", "./types"]
  },
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}
